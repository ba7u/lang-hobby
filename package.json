{
  "name": "lang",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn client:build && yarn server:build",
    "server:build": "RUN_ENV=prod nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "yarn server:dev",
    "clean": "yarn client:clean && yarn server:clean",
    "server:start": "nest start",
    "server:dev": "RUN_ENV=local API_LOG_ENABLED=true nest start --watch",
    "server:debug": "nest start --debug --watch",
    "server:prod": "RUN_ENV=prod RUN_SSL=true node dist/main",
    "server:clean": "rimraf dist",
    "client:dev": "cd src/client && next dev",
    "client:build": "cd src/client && next build",
    "client:lint": "cd src/client && next lint",
    "client:start": "cd src/client && next start",
    "client:clean": "cd src/client && rimraf .next",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prettier-check": "prettier --check",
    "prettier-fix": "prettier . --write"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "10.2.7",
    "@nestjs/core": "10.2.7",
    "@nestjs/microservices": "^10.2.7",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "10.2.7",
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.5.1",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "classnames": "^2.3.2",
    "cli-color": "^2.0.3",
    "clsx": "2.0.0",
    "cookie-parser": "1.4.6",
    "dotenv": "16.3.1",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "nest-winston": "^1.9.4",
    "next": "latest",
    "openai": "^4.12.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redis": "^4.6.10",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@tailwindcss/forms": "^0.5.6",
    "@types/cookie-parser": "1.4.4",
    "@types/express": "4.17.19",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.4",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "autoprefixer": "^10.4.16",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "postcss": "8.4.31",
    "postcss-import": "^15.1.0",
    "prettier": "3.0.3",
    "source-map-support": "0.5.21",
    "tailwindcss": "3.3.3",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,scss,md}": "prettier --check",
    "*.{js,ts,tsx}": "eslint --max-warnings=0",
    "**/*spec.{js,ts,tsx}": "yarn test"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}